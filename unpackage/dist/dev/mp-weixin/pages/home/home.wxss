page {
  background-color: #fff;
}
.page-index {
  padding-top: env(safe-area-inset-top);
  padding-bottom: env(safe-area-inset-bottom);
}
.page-index .header {
    padding: 20rpx 50rpx;
}
.page-index .header .logo {
      height: 91rpx;
      width: 397rpx;
      margin: 20rpx 0;
}
.page-index .header .desc {
      font-size: 28rpx;
      color: #909ca2;
}
.page-index .container {
    background-color: #57545f;
    padding: 20rpx 33rpx;
    box-sizing: border-box;
}
.page-index .container .item__label {
      display: inline-block;
      height: 70rpx;
      line-height: 70rpx;
      padding: 0 30rpx;
      font-size: 28rpx;
      color: #909ca2;
}
.page-index .container .item__container .block {
      display: -webkit-box;
      display: -webkit-flex;
      display: flex;
      -webkit-box-align: center;
      -webkit-align-items: center;
              align-items: center;
      height: 80rpx;
      padding: 0 30rpx;
      margin-bottom: 30rpx;
      font-size: 26rpx;
      background-color: #f7f7f7;
      border-radius: 80rpx;
}
.page-index .container .item__container .block:active {
        background-color: #eee;
}
.page-index .container .item__container .block .label {
        -webkit-box-flex: 1;
        -webkit-flex: 1;
                flex: 1;
}
.page-index .demo-button .cl-button {
    margin-bottom: 20rpx;
    position: fixed;
    bottom: 0px;
}
.page-index .fixed-0 {
    width: 25vw;
    height: 25vw;
    border-radius: 50%;
    left: 40%;
    text-align: center;
    line-height: 25vw;
    font-size: 5vw;
}
.page-index .fixed-1 {
    right: 10%;
    border-radius: 50%;
    text-align: center;
    width: 12vw;
    height: 12vw;
    line-height: 12vw;
    font-size: 3.8vw;
    background-color: #4f3b34;
}
.page-index .fixed-1 text {
    margin-left: -1.5vw;
}


<view class="{{['cl-avatar',isShape]}}" style="{{'height:'+(height)+';'+('width:'+(width)+';')}}"><block wx:if="{{!src}}"><block wx:if="{{$slots.default}}"><slot></slot></block><block wx:else><text class="cl-avatar__slot cl-icon-avatar" style="{{'font-size:'+(fontSize)+';'}}"></text></block></block><block wx:else><image class="cl-avatar__target" src="{{src}}" data-event-opts="{{[['error',[['handleError',['$event']]]],['load',[['handleLoad',['$event']]]]]}}" binderror="__e" bindload="__e"></image></block></view>